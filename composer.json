    "require": {
        "php": ">=7.3",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "eloquent/phony-phpunit": "^6.0 || ^7.1",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Ruecoder\\OAuth2\\Client\\": "src/"
        }
    },
    "scripts": {
        "check": "phpcs src test --standard=PSR12 -sp"
    }
}
