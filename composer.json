{
    "name": "ruecoder/oauth2-vk",
    "description": "VK OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Samoylov",
            "email": "admin@evilcoder.ru"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "vk"
